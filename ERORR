                                    SmartSystem.CommandHistory[#SmartSystem.CommandHistory + 1] = {
                                        type = command,
                                        target = target,
                                        params = args,
                                        time = os.time(),
                                        executed = true
                                    }
                                end
                            end
                        end
                    end
                end
            end
        else
            consecutiveErrors = consecutiveErrors + 1
            if consecutiveErrors > 5 then
                warn("âš ï¸ Ø§Ù„Ù†Ø¸Ø§Ù… ØºÙŠØ± Ù…ØªØµÙ„ Ù…Ù†Ø°", os.time() - lastSuccessTime, "Ø«Ø§Ù†ÙŠØ©")
            end
        end
        
        -- ØªØ­Ø¯ÙŠØ« Ø§Ù„Ø¥Ø­ØµØ§Ø¦ÙŠØ§Øª
        SmartSystem.SystemStats.SystemUptime = os.time() - SmartSystem.StartTime
        SmartSystem.SystemStats.PlayersTracked = #SmartSystem.CommandHistory
    end
end

