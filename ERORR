-- ğŸ§  Ø§Ù„Ù†Ø¸Ø§Ù… Ø§Ù„Ø°ÙƒÙŠ Ù„Ù„Ø§ØªØµØ§Ù„
local function SmartFetch(url, retries)
    retries = retries or 3
    
    for i = 1, retries do
        local success, response = pcall(function()
            local startTime = os.clock()
            local result = HttpService:GetAsync(url, true)
            local endTime = os.clock()
            
            return {
                data = result,
                responseTime = endTime - startTime,
                success = true,
                attempt = i
            }
        end)
        
        if success then
            return response
        elseif i < retries then
            task.wait(1 * i) -- Ø²ÙŠØ§Ø¯Ø© Ø§Ù„Ø§Ù†ØªØ¸Ø§Ø± Ù…Ø¹ ÙƒÙ„ Ù…Ø­Ø§ÙˆÙ„Ø©
        end
    end
    
    return {
        success = false,
        error = "ÙØ´Ù„ Ø¬Ù…ÙŠØ¹ Ù…Ø­Ø§ÙˆÙ„Ø§Øª Ø§Ù„Ø§ØªØµØ§Ù„",
        responseTime = 0
    }
end

-- ğŸ“¡ Ù†Ø¸Ø§Ù… Ù…Ø±Ø§Ù‚Ø¨Ø© Ø§Ù„Ø£ÙˆØ§Ù…Ø± Ø§Ù„Ø°ÙƒÙŠ
local function SmartCommandMonitor()
