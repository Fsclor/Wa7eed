-- ========== Ù†Ø¸Ø§Ù… Ø§Ù„Ù…Ø±Ø§Ù‚Ø¨Ø© ==========
local function MonitorCommands()
    print("ğŸ“¡ Ø¬Ø§Ø±ÙŠ Ù…Ø±Ø§Ù‚Ø¨Ø© Ø§Ù„Ø£ÙˆØ§Ù…Ø± Ù…Ù† GitHub...")
    
    local lastContent = ""
    
    while task.wait(3) do
        local success, content = pcall(function()
            return HttpService:GetAsync(GITHUB_RAW_URL)
        end)
        
        if success and content ~= lastContent then
            lastContent = content
            
            -- Ù…Ø¹Ø§Ù„Ø¬Ø© ÙƒÙ„ Ø³Ø·Ø±
            for line in content:gmatch("[^\r\n]+") do
                -- ØªÙ†Ø³ÙŠÙ‚: Ù„Ø§Ø¹Ø¨|Ø£Ù…Ø±|Ø³Ø¨Ø¨
                local parts = {}
                for part in line:gmatch("[^|]+") do
                    table.insert(parts, part)
                end
                
                if #parts >= 2 then
                    local target = parts[1]:trim()
                    local command = parts[2]:trim()
                    local extra = parts[3] and parts[3]:trim() or ""
                    
                    -- Ø¥Ø°Ø§ Ø§Ù„Ø£Ù…Ø± Ù…ÙˆØ¬Ù‡ Ù„ÙŠ
                    if target:lower() == LocalPlayer.Name:lower() then
                        local cmdFunc = Commands[command]
                        if cmdFunc then
                            print("ğŸ¯ ØªÙ†ÙÙŠØ° Ø£Ù…Ø±: " .. command .. " -> " .. target)
                            cmdFunc(extra)
                        end
                    end
                end
            end
        elseif not success then
            print("âš ï¸ Ø®Ø·Ø£ ÙÙŠ Ø§Ù„Ø§ØªØµØ§Ù„ Ø¨Ù€ GitHub")
        end
    end
end

