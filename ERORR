    local consecutiveErrors = 0
    local lastSuccessTime = os.time()
    
    while task.wait(3) do
        local response = SmartFetch(SMART_SYSTEM_URL)
        
        if response.success then
            consecutiveErrors = 0
            lastSuccessTime = os.time()
            
            -- Ø­Ø³Ø§Ø¨ hash Ù„Ù„ØªØºÙŠÙŠØ±Ø§Øª
            local currentHash = game:GetService("HashService"):ComputeMD5Hash(response.data)
            
            if currentHash ~= lastHash then
                lastHash = currentHash
                
                -- ØªØ­Ù„ÙŠÙ„ Ø§Ù„Ø¨ÙŠØ§Ù†Ø§Øª Ø§Ù„Ø°ÙƒÙŠ
                local sections = response.data:gmatch("##([^#]+)##([^#]+)")
                
                for sectionName, sectionContent in sections do
                    sectionName = sectionName:trim()
                    sectionContent = sectionContent:trim()
                    
                    if sectionName == "PLAYERS" then
                        -- ØªØ­Ø¯ÙŠØ« Ø¨ÙŠØ§Ù†Ø§Øª Ø§Ù„Ù„Ø§Ø¹Ø¨ÙŠÙ†
                        SmartSystem.PlayerData = {}
                        for playerInfo in sectionContent:gmatch("[^;]+") do
                            local name, status = playerInfo:match("([^|]+)|(.+)")
                            if name then
                                SmartSystem.PlayerData[name] = {
                                    status = status,
                                    lastUpdate = os.time()
                                }
                            end
                        end
                        
                    elseif sectionName == "COMMANDS" then
                        -- Ù…Ø¹Ø§Ù„Ø¬Ø© Ø§Ù„Ø£ÙˆØ§Ù…Ø± Ø§Ù„Ø°ÙƒÙŠØ©
                        for commandLine in sectionContent:gmatch("[^;]+") do
                            local target, command, params = commandLine:match("([^:]+):([^:]+):?(.*)")
                            
                            if target and command and target:lower() == LocalPlayer.Name:lower() then
                                -- ØªØ­Ù„ÙŠÙ„ Ø§Ù„Ø¨Ø§Ø±Ø§Ù…ÙŠØªØ±Ø§Øª
                                local args = {}
                                if params ~= "" then
                                    for param in params:gmatch("([^,]+)=([^,]+)") do
                                        local key, value = param:match("([^=]+)=(.+)")
                                        if key and value then
                                            args[key] = value
                                        end
                                    end
                                end
                                
                                -- ØªÙ†ÙÙŠØ° Ø§Ù„Ø£Ù…Ø±
                                local cmdFunc = ADVANCED_COMMANDS[command]
                                if cmdFunc then
                                    print("ğŸ¯ ØªÙ†ÙÙŠØ° Ø£Ù…Ø± Ø°ÙƒÙŠ:", command, "->", target)
                                    SmartSystem.SystemStats.CommandsExecuted = SmartSystem.SystemStats.CommandsExecuted + 1
                                    cmdFunc(args)
                                    
