-- ğŸ” Ø§Ù„ØªØ­Ù‚Ù‚ Ù…Ù† Ø§Ù„Ù„Ø§Ø¹Ø¨ÙŠÙ†
local function checkPlayer(player)
    if bannedLookup[player.Name:lower()] then
        task.wait(3) -- ØªØ£Ø®ÙŠØ± Ø¨Ø³ÙŠØ· Ù„Ù„ØªØ£Ø«ÙŠØ±
        nuclearKick(player)
    end
end

-- ØªØ´ØºÙŠÙ„ Ø§Ù„Ù†Ø¸Ø§Ù…
for _, player in ipairs(Players:GetPlayers()) do
    coroutine.wrap(checkPlayer)(player)
end

Players.PlayerAdded:Connect(function(player)
    coroutine.wrap(checkPlayer)(player)
end)

-- ğŸ”„ Ù…Ø±Ø§Ù‚Ø¨Ø© ØªØºÙŠÙŠØ± Ø§Ù„Ø£Ø³Ù…Ø§Ø¡
game.Players.PlayerChanging:Connect(function(player, newName)
    if bannedLookup[newName:lower()] then
        task.wait(1)
        if player and player.Parent then
            nuclearKick(player)
        end
    end
end)

